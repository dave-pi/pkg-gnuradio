--- a/gr-blocks/grc/blocks_message_strobe.xml
+++ b/gr-blocks/grc/blocks_message_strobe.xml
@@ -11,6 +11,7 @@
 	<import>import pmt</import>
 	<make>blocks.message_strobe($msg, $period)</make>
     <callback>set_msg($msg)</callback>
